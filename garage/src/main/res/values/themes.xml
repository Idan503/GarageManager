        <item name="colorPrimary">@color/blue_400</item>
        <item name="colorPrimaryVariant">@color/blue_800</item>
        <item name="colorOnPrimary">@color/blue_50</item>
        <item name="colorSecondary">@color/blue_200</item>
        <item name="colorSecondaryVariant">@color/blue_600</item>
        <item name="colorOnSecondary">@color/blue_900</item>
